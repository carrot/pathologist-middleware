{
  "name": "pathologist-middleware",
  "version": "0.0.1",
  "author": "Carrot Creative <dev@carrotcreative.com>",
  "description": "Handling custom static routes since 1967",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/carrot/pathologist-middleware.git"
  },
  "main": "lib",
  "devDependencies": {
    "mocha": "*",
    "chai": "*",
    "chai-http": "*",
    "connect": "*",
    "coffeelint": "*",
    "istanbul": "*",
    "mocha-lcov-reporter": "*",
    "coffee-script": "1.9.x",
    "alchemist-middleware": "0.0.x",
    "coveralls": "2.11.x"
  },
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "find lib -name '*.coffee' | xargs coffeelint",
    "coverage": "make build; istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage && make unbuild"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "minimatch": "2.0.x"
  }
}
